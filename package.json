{
  "name": "btd6-index-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.2.0",
    "@cloudflare/pages-plugin-cloudflare-access": "^1.0.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jose": "^4.14.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "remark-gfm": "^3.0.1",
    "web-vitals": "^2.1.4",
    "wrangler": "^3.4.0"
  },
  "scripts": {
    "start": "concurrently -k true 'npm:start-react' 'npm:start-wrangler'",
    "start-react": "npx cross-env BROWSER=none react-scripts start",
    "start-wrangler": "node --dns-result-order=ipv4first node_modules/wrangler/bin/wrangler.js pages dev --proxy 3000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3"
  }
}
